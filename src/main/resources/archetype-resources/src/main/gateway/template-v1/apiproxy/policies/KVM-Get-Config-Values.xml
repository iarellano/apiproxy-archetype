<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="KVM-Get-Config-Values" mapIdentifier="${apiproxy.canonicalName}">
    <DisplayName>KeyValueMap: Get Config Values</DisplayName>
    <Description>
        Get proxy config values
    </Description>
    <Get assignTo="keyValue" index="1">
        <Key>
            <Parameter>keyName</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
    <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
</KeyValueMapOperations>